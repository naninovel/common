    <ItemGroup>
        <PackageReference Include="DotNetJS" Version="0.13.2"/>
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="../Naninovel.Common/Naninovel.Common.csproj"/>
    </ItemGroup>

</Project>
